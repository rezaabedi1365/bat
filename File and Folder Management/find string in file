
#FIND command can scan large files line-by-line to find a certain string. It doesn't support wildcards in the search string.

@echo off

FIND "Application" d:\file1.txt 

if %ERRORLEVEL%. equ 0. (
	echo up to date word in %DATE% %TIME% Was Successful. >> D:\resault.txt
	exit /B 
)
else (
	echo up to date word in %DATE% %TIME% Was Fail. >> D:\resault.txt





